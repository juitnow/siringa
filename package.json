{
  "name": "siringa",
  "version": "0.4.1",
  "description": "Simple type-checking injection library for Typescript",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "scripts": {
    "build": "plug --force-esm",
    "coverage": "plug coverage",
    "dev": "npx nodemon -e ts -x plug -w src -w test -w test-d",
    "lint": "plug --force-esm lint",
    "test": "plug test",
    "transpile": "plug transpile"
  },
  "author": "Juit Developers <developers@juit.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@plugjs/build": "^0.6.73",
    "@plugjs/tsd": "^0.6.83",
    "typescript": "^5.9.3"
  },
  "directories": {
    "test": "test"
  },
  "homepage": "https://github.com/juitnow/siringa#readme",
  "bugs": {
    "url": "https://github.com/juitnow/siringa/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/juitnow/siringa.git"
  },
  "files": [
    "*.md",
    "dist/",
    "src/"
  ]
}
